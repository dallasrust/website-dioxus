{
  "author": "David Wallace Croft <david@CroftSoft.com>",
  "bugs": {
    "url": "https://github.com/dallasrust/website-dioxus/issues"
  },
  "description": "Dallas Rust User Meetup website using Dioxus",
  "devDependencies": {
    "http-server": "^14.1.1",
    "prettier": "3.0.2"
  },
  "homepage": "https://github.com/dallasrust/website-dioxus#readme",
  "license": "MIT",
  "name": "website-dioxus",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dallasrust/website-dioxus.git"
  },
  "scripts": {
    "prerender": "cargo run --bin prerender --features=prerender",
    "prettier": "prettier static --ignore-path .prettierignore --write",
    "serve": "http-server static -o",
    "start": "npm run prerender && npm run prettier && npm run static && npm run serve",
    "static": "cp -r public/* static"
  },
  "version": "0.0.2-SNAPSHOT"
}
